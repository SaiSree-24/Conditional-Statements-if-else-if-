# Conditional-Statements-if-else-if-
developed by SaiSree

#include <stdio.h>
int main()
{
   int m;
   printf("Enter your Marks\n");
   scanf("%d",&m);
if(m<=100 && m>=85)
{
    printf("Grade A\n");
}
else
{
     if(m<=84 && m>=70)
     {
       printf("Grade B\n");
}
else
{
    if(m<=69 && m>=55)
    {
       printf("Grade C\n");
    }
    else
   {
       if(m<=54 && m>=40)
       {
           printf("Grade D\n");
        }
        else
        {
            printf("Grade F\n");
        }
   }
}
}
return 0;
}
